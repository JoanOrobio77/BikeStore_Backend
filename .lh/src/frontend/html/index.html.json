            "date": 1743971091103,
            "name": "Commit-0",
            "content": "<!DOCTYPE html>\r\n<html lang=\"es\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Bikestore - Sistema de Gestión</title>\r\n    <link rel=\"stylesheet\" href=\"../css/styles.css\">\r\n</head>\r\n<body>\r\n    <header>\r\n        <nav>\r\n            <div class=\"logo\">Bikestore</div>\r\n            <ul class=\"nav-links\">\r\n                <li><a href=\"#\" data-section=\"productos\">Productos</a></li>\r\n                <li><a href=\"#\" data-section=\"categorias\">Categorías</a></li>\r\n                <li><a href=\"#\" data-section=\"ventas\">Ventas</a></li>\r\n                <li><a href=\"#\" data-section=\"stock\">Stock</a></li>\r\n            </ul>\r\n            <div class=\"user-info\">\r\n                <span id=\"userName\"></span>\r\n                <button id=\"logoutBtn\">Cerrar Sesión</button>\r\n            </div>\r\n        </nav>\r\n    </header>\r\n\r\n    <main>\r\n        <!-- Sección de Login -->\r\n        <section id=\"loginSection\" class=\"active\">\r\n            <h2>Iniciar Sesión</h2>\r\n            <form id=\"loginForm\">\r\n                <div class=\"form-group\">\r\n                    <label for=\"email\">Email:</label>\r\n                    <input type=\"email\" id=\"email\" required>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"password\">Contraseña:</label>\r\n                    <input type=\"password\" id=\"password\" required>\r\n                </div>\r\n                <button type=\"submit\">Iniciar Sesión</button>\r\n            </form>\r\n        </section>\r\n\r\n        <!-- Sección de Productos -->\r\n        <section id=\"productosSection\" class=\"hidden\">\r\n            <h2>Gestión de Productos</h2>\r\n            <button id=\"addProductoBtn\">Agregar Producto</button>\r\n            <div id=\"productosList\"></div>\r\n        </section>\r\n\r\n        <!-- Sección de Categorías -->\r\n        <section id=\"categoriasSection\" class=\"hidden\">\r\n            <h2>Gestión de Categorías</h2>\r\n            <button id=\"addCategoriaBtn\">Agregar Categoría</button>\r\n            <div id=\"categoriasList\"></div>\r\n        </section>\r\n\r\n        <!-- Sección de Ventas -->\r\n        <section id=\"ventasSection\" class=\"hidden\">\r\n            <h2>Gestión de Ventas</h2>\r\n            <button id=\"addVentaBtn\">Nueva Venta</button>\r\n            <div id=\"ventasList\"></div>\r\n        </section>\r\n\r\n        <!-- Sección de Stock -->\r\n        <section id=\"stockSection\" class=\"hidden\">\r\n            <h2>Gestión de Stock</h2>\r\n            <div id=\"stockList\"></div>\r\n        </section>\r\n    </main>\r\n\r\n    <!-- Modales -->\r\n    <div id=\"modal\" class=\"modal hidden\">\r\n        <div class=\"modal-content\">\r\n            <span class=\"close\">&times;</span>\r\n            <div id=\"modalContent\"></div>\r\n        </div>\r\n    </div>\r\n\r\n    <script src=\"../scripts/auth.js\"></script>\r\n    <script src=\"../scripts/productos.js\"></script>\r\n    <script src=\"../scripts/categorias.js\"></script>\r\n    <script src=\"../scripts/ventas.js\"></script>\r\n    <script src=\"../scripts/stock.js\"></script>\r\n    <script src=\"../scripts/main.js\"></script>\r\n</body>\r\n</html> "
